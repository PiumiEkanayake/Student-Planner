    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="2dp"

        android:layout_marginTop="2dp">


        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@color/col3"

            >
            <TextView
                android:id="@+id/eName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:paddingTop="5dp"
                android:paddingLeft="15dp"
                android:paddingBottom="15dp"
                android:paddingRight="15dp"
                android:text="MAD Online"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:textSize="20dp" />


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="6dp">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="35dp"
                    android:src="@drawable/subject"
                    android:tint="@color/white"




                    />

                <TextView
                    android:id="@+id/sirName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:paddingTop="29dp"
                    android:paddingLeft="25dp"
                    android:paddingBottom="10dp"
                    android:paddingRight="5dp"
                    android:text="MAD"
                    android:textColor="@color/white"

                    android:textSize="14dp" />

                <ImageView
                    android:id="@+id/classIcon"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_toRightOf="@id/sirName"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="33dp"
                    android:src="@drawable/classroom"
                    android:tint="@color/white"

                    />
                <TextView
                    android:id="@+id/eventName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/classIcon"
                    android:gravity="start"

                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="30dp"




                    android:text="A102"
                    android:textColor="@color/white"

                    android:textSize="13dp" />
                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"

                    android:layout_marginStart="120dp"

                    android:layout_marginTop="2dp"
                    android:background="@drawable/dashboard_mini_icon"
                    android:paddingLeft="15dp"
                    android:paddingTop="2dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="2dp"
                    android:text="Exam"
                    android:textColor="@color/blue"
                    />

            </RelativeLayout>
            <TextView
                android:id="@+id/sTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"

                android:layout_marginTop="5dp"
                android:paddingBottom="0dp"
                android:paddingRight="35dp"
                android:paddingTop="5dp"
                android:text="30"
                android:textColor="@color/white"
                android:textSize="16dp"
                android:textStyle="bold"/>
            <TextView
                android:id="@+id/eTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_below="@id/sTime"
                android:layout_alignParentRight="true"
                android:paddingTop="0dp"
                android:paddingRight="15dp"
                android:paddingBottom="0dp"

                android:text="mins to go"
                android:textColor="@color/white"
                android:textSize="12dp"
                android:textStyle="bold"/>

        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="2dp"

        android:layout_marginTop="2dp">


        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@color/col2"

            >
            <TextView
                android:id="@+id/eName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:paddingTop="5dp"
                android:paddingLeft="15dp"
                android:paddingBottom="15dp"
                android:paddingRight="15dp"
                android:text="Physics"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:textSize="20dp" />


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="6dp">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="35dp"
                    android:src="@drawable/teacher"
                    android:tint="@color/white"




                    />

                <TextView
                    android:id="@+id/sirName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:paddingTop="29dp"
                    android:paddingLeft="25dp"
                    android:paddingBottom="10dp"
                    android:paddingRight="5dp"
                    android:text="Sir Einstein"
                    android:textColor="@color/white"

                    android:textSize="14dp" />

                <ImageView
                    android:id="@+id/classIcon"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_toRightOf="@id/sirName"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="33dp"
                    android:src="@drawable/classroom"
                    android:tint="@color/white"

                   />
                <TextView
                    android:id="@+id/eventName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/classIcon"
                    android:gravity="start"

                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="30dp"




                    android:text="A102"
                    android:textColor="@color/white"

                    android:textSize="13dp" />
                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"

                    android:layout_marginStart="90dp"

                    android:layout_marginTop="2dp"
                    android:background="@drawable/dashboard_mini_icon"
                    android:paddingLeft="15dp"
                    android:paddingTop="2dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="2dp"
                    android:text="Class"
                    android:textColor="@color/blue"
                   />

            </RelativeLayout>
            <TextView
                android:id="@+id/sTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"

                android:layout_marginTop="5dp"
                android:paddingBottom="0dp"
                android:paddingRight="15dp"
                android:paddingTop="5dp"
                android:text="09:30 A.M"
                android:textColor="@color/white"
                android:textSize="16dp"
                android:textStyle="bold"/>
            <TextView
                android:id="@+id/eTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_below="@id/sTime"
                android:layout_alignParentRight="true"
                android:paddingTop="0dp"
                android:paddingRight="15dp"
                android:paddingBottom="0dp"

                android:text="10:30 A.M"
                android:textColor="@color/white"
                android:textSize="16dp"
                android:textStyle="bold"/>

        </RelativeLayout>

    </LinearLayout>


</LinearLayout>